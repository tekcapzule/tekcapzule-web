{
  "name": "tekcapsule-web",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:deps": "npm install --force",
    "start:dev": "ng serve --host 0.0.0.0",
    "start:hmr": "ng serve --hmr",
    "start:win": "SET NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build:prod": "ng build --prod",
    "build:staging": "ng build --configuration=staging",
    "prettify": "prettier --config ./.prettierrc --write 'src/app/**/*.{js,ts,json,css,scss,html}'",
    "test:headless": "ng test --browsers=ChromeHeadless --reporters=spec",
    "test:headless:sr": "ng test --browsers=ChromeHeadless --reporters=spec --watch=false"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|ts|json|css|scss|html)'"
    }
  },
  "dependencies": {
    "@angular/animations": "~11.2.9",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.2.9",
    "@angular/compiler": "~11.2.9",
    "@angular/core": "~11.2.9",
    "@angular/forms": "~11.2.9",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.2.9",
    "@angular/platform-browser-dynamic": "~11.2.9",
    "@angular/router": "~11.2.9",
    "@aws-amplify/ui-angular": "^1.0.17",
    "angular-responsive-carousel": "^2.1.2",
    "aws-amplify": "^4.2.2",
    "aws-amplify-angular": "^6.0.8",
    "moment": "^2.29.4",
    "ngx-chips": "2.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.2.4",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "~11.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "^0.0.32",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
