{
  "name": "tekcapzule-web",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:deps": "npm install --force",
    "start:dev": "ng serve --host 0.0.0.0",
    "start:hmr": "ng serve --hmr",
    "start:win": "SET NODE_OPTIONS=--openssl-legacy-provider && ng serve",
    "build:prod": "ng build --configuration production",
    "build:staging": "ng build --configuration=staging",
    "prettify": "prettier --config ./.prettierrc --write 'src/app/**/*.{js,ts,json,css,scss,html}'",
    "test:headless": "ng test --browsers=ChromeHeadless --reporters=spec",
    "test:headless:sr": "ng test --browsers=ChromeHeadless --reporters=spec --watch=false"
  },
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|ts|json|css|scss|html)'"
    }
  },
  "dependencies": {
    "@angular/animations": "15.2.5",
    "@angular/cdk": "15.2.5",
    "@angular/common": "15.2.5",
    "@angular/compiler": "15.2.5",
    "@angular/core": "15.2.5",
    "@angular/forms": "15.2.5",
    "@angular/material": "15.2.5",
    "@angular/platform-browser": "15.2.5",
    "@angular/platform-browser-dynamic": "15.2.5",
    "@angular/router": "15.2.5",
    "@aws-amplify/ui-angular": "^3.2.12",
    "aws-amplify": "^5.3.10",
    "d3": "^7.8.5",
    "moment": "2.29.4",
    "primeicons": "^6.0.1",
    "primeng": "^15.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular/cli": "15.2.4",
    "@angular/compiler-cli": "15.2.5",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "3.6.0",
    "@types/node": "12.11.1",
    "codelyzer": "6.0.0",
    "husky": "6.0.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "5.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.0",
    "karma-jasmine-html-reporter": "1.5.0",
    "karma-spec-reporter": "0.0.32",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "protractor": "7.0.0",
    "ts-node": "8.3.0",
    "tslint": "6.1.0",
    "typescript": "4.9.5"
  }
}
